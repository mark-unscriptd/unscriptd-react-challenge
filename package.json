{
  "name": "react-coding-challenge",
  "version": "1.0.0",
  "description": "A package containing a simple dummy media API, which should be used in the challenge",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "json-server --port 3010 --watch db.json",
    "dev": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "keywords": [
    "react challenge",
    "tryouts",
    "react developer",
    "hiring"
  ],
  "author": "Unscriptd Ltd",
  "license": "ISC",
  "devDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "json-server": "0.10.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "node-sass-chokidar": "^1.1.0",
    "react-router-dom": "^4.2.2"
  }
}
